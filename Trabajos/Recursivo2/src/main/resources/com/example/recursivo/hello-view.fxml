<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox alignment="CENTER" prefHeight="500.0" prefWidth="500.0" spacing="20.0" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.recursivo.HelloController">
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
    </padding>
   <children>
      <Label text="Buscar">
         <font>
            <Font size="26.0" />
         </font>
      </Label>
      <TextField fx:id="txtBuscar" />
      <Button mnemonicParsing="false" onAction="#buscar" text="Button" />
      <Label fx:id="lblResultado" text="Label" />
      <Label text="Encriptar">
         <font>
            <Font size="35.0" />
         </font>
      </Label>
      <TextField fx:id="txtEncriptar" />
      <Button mnemonicParsing="false" onAction="#encriptar" text="Encriptar" />
      <Label fx:id="lblResultadoEnc" text="Label" />
   </children>
</VBox>
